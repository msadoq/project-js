    "build:main": "node -r babel-register -r ./lib/common/initParameters.js ./node_modules/.bin/webpack --config webpack/config.main.js --progress --profile --colors",
    "build:renderer": "NODE_ENV=production node -r babel-register -r ./lib/common/initParameters.js ./node_modules/.bin/webpack --config webpack/config.renderer.production.js --progress --profile --colors",
    "build:server": "node -r babel-register -r ./lib/common/initParameters.js ./node_modules/.bin/webpack --config webpack/config.server.js --progress --profile --colors",
    "clean": "rimraf ./dist stats.json ./coverage ./scripts/lint-reporter/*.csv",
    "clean:cache": "rimraf .eslintcache",
    "clone:gpds": "./scripts/cloneGPDS.sh",
    "hello": "npm run clone:gpds && npm run link && npm run install:adapters && npm install && [ -f config.local.json ] && echo || cp config.sample.json config.local.json",
    "hello:no-rtd": "npm run link:common && npm run install:adapters && npm install && [ -f config.local.json ] && echo || cp config.sample.json config.local.json",
    "postinstall": "electron-rebuild && npm run install:rtd && npm run install:adapters",
    "install:adapters": "node ./scripts/adaptersInstall.js",
    "install:devtools": "./scripts/devtoolsInstall.sh",
    "install:rtd": "node ./scripts/rtdInstall.js",
    "link": "npm run link:common && npm run link:rtd",
    "link:common": "npm link ../../../../../common/src/main/js/common && (cd ../../../../../common/src/main/js/common && npm install)",
    "link:rtd": "npm link ../../../../../../../../GPDS/src/api/js/rtd/src/main/js/rtd/ && (cd ../../../../../../../../GPDS/src/api/js/rtd/src/main/js/rtd/ && npm install)",
    "lint": "eslint --max-warnings 0 . --cache",
    "lint:isis": "eslint --max-warnings 0 -c .eslintrc_isis.yml .",
    "lint:report": "node -r babel-register scripts/lint-reporter/lint-reporter.js",
    "open:test:coverage": "firefox coverage/test/lcov-report/index.html",
    "package:electron": "cd dist && electron-packager . --no-prune --overwrite",
    "precommit": "lint-staged",
    "prepush": "npm run push-gitlab-ci",
    "prepare:maven:build": "node ./scripts/prepareMavenBuild.js",
    "sh": "npm run start-hot",
    "staged-precommit-hook": "unset GIT_DIR; ./scripts/staged-precommit-hook.sh",
    "push-gitlab-ci": "./scripts/push-gitlab-hook.sh",
    "start": "./scripts/start.sh",
    "start-hot": "APP_ENV=main HOT=1 electron -r babel-register -r babel-polyfill ./main.development",
    "start:dc:stub": "LOG=console ZMQ_GPCCDC_PUSH=tcp://127.0.0.1:5042 ZMQ_GPCCDC_PULL=tcp://127.0.0.1:5043 ISIS_DOCUMENTS_ROOT=/data/work/gitRepositories/LPISIS/GPCCHS/GPCCHS_E_VIS/src/client/src/main/js/client/data WORKSPACE=dev.workspace.viws node ./lib/stubProcess/dc.js",
    "test": "NODE_ENV=test jest",
    "test:adapters": "(cd ./adapters/dc && npm test) && (cd ./adapters/isis && npm test)",
    "test:all": "npm run test:adapters && npm test",
    "test:coverage": "npm test -- --coverage --no-cache",
    "test:pic": "NODE_ENV=test jest --ci --testResultsProcessor jest-junit-reporter",
    "test:watch": "npm test -- --watch",
    "test:validateViewConfigurationFiles": "./scripts/validateViewConfigurationFiles.js",
    "tw": "npm test -- --watch",
    "webpack": "node -r babel-register -r ./lib/common/initParameters.js webpack/server.js",
    "wp": "npm run webpack",
    "zip:coverage": "(cd coverage/test && zip -r ../coverage-report.zip ./lcov-report)",
    "zip:lint:report": "(cd scripts/lint-reporter && zip ./lint-report.zip *.csv)"
  },
  "jest": {
    "collectCoverageFrom": [
      "lib/**/*.js"
    ],
    "coverageDirectory": "coverage/test",
    "moduleNameMapper": {
      "\\.(less|css|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./lib/common/jest/polyfill.js"
    ],
    "setupTestFrameworkScriptFile": "./lib/common/jest/setup.js",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/adapters/"
    ],
    "testRegex": "(spec)\\.(js|jsx)$",
    "transformIgnorePatterns": [
      "node_modules/(?!common)|(node_modules/common/node_modules)"
    ],
    "verbose": true
  },
