  "main": "main.js",
  "jest": {
    "verbose": true,
    "testRegex": "(snapshot)\\.(js|jsx)$",
    "moduleNameMapper": {
      "\\.(less|css)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "lib/windowProcess/**/*.js",
      "VIVL**/*.js"
    ],
    "coverageDirectory": "coverage/snapshot"
  },
  "scripts": {
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "node -r babel-register -r ./lib/common/initParameters.js ./node_modules/.bin/webpack --config webpack/config.main.js --progress --profile --colors",
    "build:renderer": "NODE_ENV=production node -r babel-register -r ./lib/common/initParameters.js ./node_modules/.bin/webpack --config webpack/config.renderer.production.js --progress --profile --colors",
    "stats:main": "BUNDLE_STATS=1 ANALYZER_PORT=4242 npm run build:main",
    "stats:main": "ANALYZER_PORT=4242 NODE_ENV=production node -r babel-register -r ./lib/common/initParameters.js ./node_modules/.bin/webpack --config webpack/stats.main.js --progress --profile --colors",
    "stats:renderer": "ANALYZER_PORT=4243 NODE_ENV=production node -r babel-register -r ./lib/common/initParameters.js ./node_modules/.bin/webpack --config webpack/stats.renderer.js --progress --profile --colors",
    "clean": "rimraf ./dist ./main.js ./main.js.map",
    "hello": "npm run link && npm install && [ -f config.local.json ] && echo || cp config.sample.json config.local.json",
    "link": "npm link ../../../../../common/src/main/js/common && npm link ../../../../../server/src/main/js/server && (cd ../../../../../server/src/main/js/server && npm run hello)",
    "lint": "eslint lib test *.js",
    "sh": "npm run start-hot",
    "start": "electron ./",
    "start-hot": "HOT=1 electron -r babel-register -r babel-polyfill ./main.development",
    "test": "NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --compilers js:babel-register \"+(lib|VIVL)/**/*.spec.js\"",
    "test:watch": "npm test -- --watch",
    "test:coverage": "(time nyc --require babel-register npm test || true) && npm run open:test-coverage",
    "open:test-coverage": "firefox coverage/test/lcov-report/index.html",
    "snapshot": "NODE_ENV=snapshot BABEL_DISABLE_CACHE=1 jest",
    "snapshot:coverage": "(npm run snapshot -- --coverage || true) && npm run open:snapshot-coverage",
    "open:snapshot-coverage": "firefox coverage/snapshot/lcov-report/index.html",
    "snapshot:watch": "npm run snapshot -- --watch",
    "snapshot:clean": "jest -u",
    "tw": "npm test -- --watch",
    "wp": "npm run webpack",
    "webpack": "node -r babel-register -r ./lib/common/initParameters.js webpack/server.js"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
