{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "minLength": 1
    },
    "title": {
      "type": "string",
      "minLength": 1
    },
    "links": {
      "type": "array",
      "items": {
        "required": [],
        "properties": {}
      }
    },
    "defaultRatio": {
      "type": "object",
      "properties": {
        "length": {
          "type": "number"
        },
        "width": {
          "type": "number"
        }
      },
      "required": [
        "length",
        "width"
      ]
    },
    "entryPoints": {
      "type": "array",
      "uniqueItems": true,
      "minItems": 1,
      "items": {
        "required": [
          "name"
        ],
        "properties": {
          "connectedData": {
            "type": "object",
            "properties": {
              "domain": {
                "type": "string",
                "minLength": 1
              },
              "formula": {
                "type": "string",
                "minLength": 1
              },
              "timeline": {
                "type": "string",
                "minLength": 1
              }
            },
            "required": [
              "domain",
              "formula",
              "timeline"
            ]
          },
          "name": {
            "type": "string",
            "minLength": 1
          }
        }
      }
    },
    "allColumns": {
      "type": "object",
      "properties": {},
      "required": []
    },
    "sorting": {
      "type": "object",
      "properties": {},
      "required": []
    },
    "hiddenColumns": {
      "type": "object",
      "properties": {},
      "required": []
    }
  },
  "required": [
    "type",
    "title"
  ]
}
