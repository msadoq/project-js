    "sh": "npm run start-hot",
    "start": "./scripts/start.sh",
    "start-hot": "APP_ENV=main HOT=1 electron -r babel-register -r babel-polyfill ./main.development",
    "start:dc:stub": "LOG=console ZMQ_GPCCDC_PUSH=tcp://127.0.0.1:5042 ZMQ_GPCCDC_PULL=tcp://127.0.0.1:5043 ISIS_DOCUMENTS_ROOT=/data/work/gitRepositories/LPISIS/GPCCHS/GPCCHS_E_VIS/src/client/src/main/js/client/data WORKSPACE=dev.workspace.json node ./lib/stubProcess/dc.js",
    "test": "NODE_ENV=test jest",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage && npm run open:test:coverage",
    "open:test:coverage": "firefox coverage/test/lcov-report/index.html",
    "tw": "npm test -- --watch",
    "wp": "npm run webpack",
    "install:adapters": "node ./scripts/adaptersInstall.js",
    "install:rtd": "node ./scripts/rtdInstall.js",
    "prepare:maven:build": "node ./scripts/prepareMavenBuild.js",
    "postinstall": "electron-rebuild && npm run install:rtd && npm run install:adapters",
    "webpack": "node -r babel-register -r ./lib/common/initParameters.js webpack/server.js",
    "benchmark:grizzly": "HOT=1 electron -r babel-register -r babel-polyfill benchmarks/grizzly.benchmark.js",
    "benchmark:textview": "HOT=1 electron -r babel-register -r babel-polyfill benchmarks/textview.benchmark.js"
