{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "minLength": 1
    },
    "title": {
      "type": "string",
      "minLength": 1
    },
    "titleStyle": {
      "type": "object",
      "properties": {
        "align": {
          "type": "string",
          "minLength": 1
        },
        "bold": {
          "type": "boolean"
        },
        "color": {
          "type": "string",
          "minLength": 1
        },
        "font": {
          "type": "string",
          "minLength": 1
        },
        "italic": {
          "type": "boolean"
        },
        "size": {
          "type": "number"
        },
        "strikeOut": {
          "type": "boolean"
        },
        "underline": {
          "type": "boolean"
        }
      }
    },
    "links": {
      "type": "array",
      "items": {
        "required": [],
        "properties": {}
      }
    },
    "defaultRatio": {
      "type": "object",
      "properties": {
        "length": {
          "type": "number"
        },
        "width": {
          "type": "number"
        }
      },
      "required": [
        "length",
        "width"
      ]
    },
    "search": {
      "type": "object",
      "properties": {
        "enabled": {
          "type": "boolean"
        }
      },
      "required": [
        "enabled"
      ]
    },
    "entryPoint": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string",
          "minLength": 1
        },
        "connectedData": {
          "type": "object",
          "properties": {
            "domain": {
              "type": "string",
              "minLength": 1
            },
            "timeline": {
              "type": "string",
              "minLength": 1
            },
            "mode": {
              "type": "number"
            }
          },
          "required": [
            "domain",
            "timeline",
            "mode"
          ]
        }
      },
      "required": [
        "name",
        "connectedData"
      ]
    }
  },
  "required": [
    "type",
    "title"
  ]
