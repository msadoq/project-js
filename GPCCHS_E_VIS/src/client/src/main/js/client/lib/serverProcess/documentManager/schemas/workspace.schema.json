{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "definitions": {
        "geometry": {
            "type": "object",
            "properties": {
                "kind": {
                    "type": "string",
                    "default": "Relative",
                    "enum": [
                        "Relative",
                        "Absolute"
                    ]
                },
                "x": {
                    "type": "number"
                },
                "y": {
                    "type": "number"
                },
                "w": {
                    "type": "number"
                },
                "h": {
                    "type": "number"
                }
            },
            "required": [
                "x",
                "y",
                "w",
                "h"
            ],
            "additionalProperties": false
        },
        "docLocation": {
            "oneOf": [
                {
                    "properties": {
                        "oId": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "oId"
                    ]
                },
                {
                    "properties": {
                        "path": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "path"
                    ]
                }
            ]
        },
        "boundaries": {
            "type": "object",
            "properties": {
                "lower": {
                    "type": "number"
                },
                "upper": {
                    "type": "number"
                }
            },
            "required": [
                "lower",
                "upper"
            ]
        },
        "timeline": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "offset": {
                    "type": "number",
                    "default": 0
                }
            },
            "required": [
                "id",
                "offset"
            ]
        },
        "sessionLine": {
            "allOf": [
                {
                    "$ref": "#/definitions/timeline"
                },
                {
                    "properties": {
                        "kind": {
                            "type": "string",
                            "const": "Session"
                        },
                        "sessionId": {
                            "type": "number"
                        }
                    },
                    "required": [
                        "kind",
                        "sessionId"
                    ]
                }
            ]
        },
        "datasetLine": {
            "allOf": [
                {
                    "$ref": "#/definitions/timeline"
                },
                {
                    "properties": {
                        "kind": {
                            "type": "string",
                            "const": "Dataset"
                        },
                        "dsPath": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "kind",
                        "dsPath"
                    ]
                }
            ]
        },
        "recordsetLine": {
            "allOf": [
                {
                    "$ref": "#/definitions/timeline"
                },
                {
                    "properties": {
                        "kind": {
                            "type": "string",
                            "const": "Recordset"
                        },
                        "rsPath": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "kind",
                        "rsPath"
                    ]
                }
            ]
        },
        "timeBar": {
            "type": "object",
            "properties": {
                "type": {
                    "type": "string",
                    "const": "timeBarConfiguration"
                },
                "id": {
                    "type": "string"
                },
                "mode": {
                    "type": "string",
                    "default": "Normal",
                    "enum": [
                        "Normal",
                        "Extended",
                        "Sliding"
                    ]
                },
                "rulerResolution": {
                    "type": "number"
                },
                "speed": {
                    "type": "number",
                    "default": 1
                },
                "timeSpec": {
                    "type": "string",
                    "const": "LocalTime"
                },
                "offsetFromUTC": {
                    "type": "number",
                    "default": 0
                },
                "timelines": {
                    "type": "array"
                },
                "masterId": {
                    "type": ["string", "null"]
                }
            },
            "required": [
                "type",
                "id",
                "mode",
                "rulerResolution",
                "timelines"
            ]
        },
        "documentWindow": {
            "type": "object",
            "properties": {
                "type": {
                    "type": "string",
                    "const": "documentWindow"
                },
                "title": {
                    "type": "string"
                },
                "pages": {
                    "type": "array",
                    "items": {
                        "allOf": [
                            {
                                "$ref": "#/definitions/docLocation"
                            },
                            {
                                "properties": {
                                    "timebarId": {
                                        "type": "string"
                                    }
                                },
                                "required": [
                                    "timebarId"
                                ]
                            }
                        ]
                    }
                },
                "geometry": {
                    "$ref": "#/definitions/geometry"
                }
            },
            "required": [
                "type",
                "geometry",
                "title"
            ],
            "additionalProperties": false
        },
        "globalViewWindow": {
            "type": "object",
            "properties": {
                "type": {
                    "type": "string",
                    "const": "globalViewWindow"
                },
                "kind": {
                    "type": "string",
                    "enum": [
                        "GMAView",
                        "OBAView"
                    ]
                },
                "timebarId": {
                    "type": "string"
                },
                "geometry": {
                    "$ref": "#/definitions/geometry"
                }
            },
            "required": [
                "type",
                "kind",
                "timebarId",
                "geometry"
            ],
            "additionalProperties": false
        },
        "sideWindow": {
            "type": "object",
            "properties": {
                "type": {
                    "type": "string",
                    "const": "sideWindow"
                },
                "kind": {
                    "type": "string",
                    "enum": [
                        "DocumentExplorer",
                        "RTDExplorer",
                        "InspectorView",
                        "Console",
                        "InputErrors"
                    ]
                },
                "geometry": {
                    "$ref": "#/definitions/geometry"
                }
            },
            "required": [
                "type",
                "kind",
                "geometry"
            ],
            "additionalProperties": false
        }
    },
    "type": "object",
    "properties": {
        "type": {
            "type": "string",
            "const": "WorkSpace"
        },
        "windows": {
            "type": "array",
            "items": {
                "oneOf": [
                    {
                        "$ref": "#/definitions/documentWindow"
                    },
                    {
                        "$ref": "#/definitions/globalViewWindow"
                    },
                    {
                        "$ref": "#/definitions/sideWindow"
                    }
                ]
            }
        },
        "timebars": {
            "type": "array",
            "items": {
                "$ref": "#/definitions/timeBar"
            }
        }
    },
    "required": [
        "windows",
        "timebars"
    ]
}