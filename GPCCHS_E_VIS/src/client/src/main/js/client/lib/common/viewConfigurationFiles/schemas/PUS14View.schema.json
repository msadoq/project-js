{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "PUS14View.schema.json",
  "description": "",
  "type": "object",
  "properties": {
    "version": {
      "type": "string",
      "minLength": 1
    },
    "type": {
      "type": "string",
      "minLength": 1
    },
    "title": {
      "type": "string",
      "minLength": 1
    },
    "titleStyle": {
      "type": "object",
      "properties": {
        "align": {
          "type": "string",
          "minLength": 1
        },
        "bold": {
          "type": "boolean"
        },
        "font": {
          "type": "string",
          "minLength": 1
        },
        "italic": {
          "type": "boolean"
        },
        "size": {
          "type": "number"
        },
        "strikeOut": {
          "type": "boolean"
        },
        "underline": {
          "type": "boolean"
        }
      },
      "required": [
        "align",
        "bold",
        "font",
        "italic",
        "size",
        "strikeOut",
        "underline"
      ]
    },
    "links": {
      "type": "array",
      "items": {
        "properties": {}
      }
    },
    "defaultRatio": {
      "type": "object",
      "properties": {
        "length": {
          "type": "number"
        },
        "width": {
          "type": "number"
        }
      },
      "required": [
        "length",
        "width"
      ]
    },
    "entryPoint": {
      "type": "object",
      "properties": {
        "connectedData": {
          "type": "object",
          "properties": {
            "formula": {
              "type": "string",
              "minLength": 1
            },
            "domain": {
              "type": "string",
              "minLength": 1
            },
            "session": {
              "type": "string",
              "minLength": 1
            },
            "apids": {
              "type": "array",
              "uniqueItems": true,
              "minItems": 1,
              "items": {
                "required": [
                  "apidName",
                  "apidRawValue"
                ],
                "properties": {
                  "apidName": {
                    "type": "string",
                    "minLength": 1
                  },
                  "apidRawValue": {
                    "type": "string",
                    "minLength": 1
                  }
                }
              }
            },
            "apidName": {
              "type": "string",
              "minLength": 1
            }
          },
          "required": [
            "formula",
            "domain",
            "session",
            "apids",
            "apidName"
          ]
        },
        "name": {
          "type": "string",
          "minLength": 1
        }
      },
      "required": [
        "connectedData",
        "name"
      ]
    },
    "tables": {
      "type": "object",
      "properties": {
        "packetForwarding": {
          "type": "object",
          "properties": {
            "cols": {
              "type": "array",
              "uniqueItems": true,
              "minItems": 1,
              "items": {
                "required": [
                  "title",
                  "displayed"
                ],
                "properties": {
                  "label": {
                    "type": "string"
                  },
                  "title": {
                    "type": "string",
                    "minLength": 1
                  },
                  "displayed": {
                    "type": "boolean"
                  }
                }
              }
            }
          },
          "required": [
            "cols"
          ]
        }
      },
      "required": [
        "packetForwarding"
      ]
    }
  },
  "required": [
    "version",
    "type",
    "title",
    "titleStyle",
    "links",
    "defaultRatio",
    "domainName",
    "sessionName",
    "entryPoint",
    "tables"
  ]
}
