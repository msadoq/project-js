{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "info": "LEVEL='INFO'",
    "debug": "LEVEL='DEBUG'",
    "verbose": "LEVEL='VERBOSE'",
    "info-all": "DEBUG='gpcchs_d_svr:server:*' npm run info",
    "debug-all": "DEBUG='gpcchs_d_svr:server:*' npm run debug",
    "verbose-all": "DEBUG='gpcchs_d_svr:server:*' npm run verbose",
    "start": "node ./bin/www",
    "start-info": "npm run info-all npm run start",
    "start-debug": "npm run debug-all npm run start",
    "start-verbose": "npm run verbose-all npm run start",
    "test": "node_modules/.bin/mocha test --recursive",
    "gpccdc-stub": "node stub/gpccdc.js -p 1",
    "send-sub-stub": "node stub/sendSubscriptions.js",
    "start-stub-only": "concurrently \"npm run info-all npm run gpccdc-stub\" \"npm run start-debug\"",
    "start-stub": "concurrently \"npm run gpccdc-stub\" \"npm run start\" \"npm run send-sub-stub\"",
    "start-stub-info": "concurrently \"npm run info-all npm run gpccdc-stub\" \"npm run start-info\" \"npm run info-all npm run send-sub-stub\""
  },
  "dependencies": {
    "body-parser": "~1.15.1",
    "chai-properties": "^1.2.1",
    "cookie-parser": "~1.4.3",
    "debug": "^2.2.0",
    "express": "~4.13.4",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "protobufjs": "^5.0.1",
    "serve-favicon": "~2.3.0",
    "socket.io": "^1.4.8",
    "supertest": "^1.2.0",
    "zmq": "^2.15.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chi": "0.0.10",
    "concurrently": "^2.2.0",
    "eslint": "^3.0.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "exec": "^0.2.1",
    "lokijs": "^1.4.1",
    "minimist": "^1.2.0",
    "mocha": "^2.5.3"
  }
}
