{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "DataFullName": {
            "type": "string"
        },
        "Field": {
            "type":"string"
        },
        "DomainId": {
            "type": "integer"
        },
        "TimeLineType": {
            "type": "string",
            "enum": [
                "dataset",
                "recordset",
                "session"
            ]
        },
        "SesionId": {
            "type": "integer"
        },
        "SetFileName": {
            "type": "string"
        },
        "SubscriptionState": {
            "type": "string",
            "enum": [
                "Pause",
                "Play"
            ]
        },
        "VisuSpeed": {
            "type": "integer"
        },
        "visuWindow": {
            "type": "object",
            "properties": {
                "lower": {
                    "type": "dateTime"
                },
                "upper": {
                    "type": "dateTime"
                }
            },
            "required": [
                "lower",
                "upper"
            ]
        },
        "Filter": {
            "type": "string"
        }
  },
  "required": [
      "DataFullName",
      "DomainId",
      "TimeLineType",
      "SubscriptionState",
      "visuWindow"
  ]
}