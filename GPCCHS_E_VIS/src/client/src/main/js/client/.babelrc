{
  "presets": [
    "es2015",
    "stage-0",
    "react"
  ],
  "plugins": [
  	"babel-plugin-add-module-exports"
  ],
  "env": {
    "production": {
      "presets": [
        "react-optimize"
      ],
      "plugins": [
        "babel-plugin-lodash",
    		"transform-class-properties"
      ]
    },
    "development": {
      "presets": [
        "react-hmre"
      ]
    }
  }
}
